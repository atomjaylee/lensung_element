.ls_selector-item {
  font-size: 28rpx;
  color: #333;
}
.ls_selector-item-disabled {
  color: rgba(51, 51, 51, 0.3);
}
.ls_selector-item-inner {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  padding-right: 24rpx;
  border-top: 1rpx solid #f7f7f7;
}
.ls_selector-item-inner text {
  flex: 1;
}
.ls_selector-item-children {
  padding-left: 24rpx;
}
